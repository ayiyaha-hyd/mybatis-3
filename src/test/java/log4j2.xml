<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">
<!--      <PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n"/>-->
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %5p [%15.15t] [%40.40c:%-4L] - %m%n"/>
    </Console>
  </Appenders>
  <Loggers>
<!--    <Logger name="org.apache.ibatis" level="DEBUG"/>-->
<!--    <Logger name="com.hyd.ayiyaha" level="DEBUG"/>-->
    <Root level="DEBUG">
      <AppenderRef ref="STDOUT"/>
    </Root>
  </Loggers>
</Configuration>
