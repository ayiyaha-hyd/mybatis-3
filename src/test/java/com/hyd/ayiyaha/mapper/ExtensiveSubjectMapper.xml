<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hyd.ayiyaha.mapper.ExtensiveSubjectMapper">
  <select id="selectAll" resultType="com.hyd.ayiyaha.entity.ExtensiveSubject">
    SELECT * FROM extensive_subject
  </select>
  <select id="selectOne" resultType="com.hyd.ayiyaha.entity.ExtensiveSubject">
    SELECT * FROM extensive_subject where aBlob = #{aBlob}
  </select>
</mapper>
